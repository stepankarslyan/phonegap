	id="com.aupa.sample"
	version="1.0.0">

	<name>sample mobile App with GPS</name>

	<description>
    sample web App with java script and html
	</description>

	<author href="http://lev90.github.com"
    email="levon.hakopyan.gmail.com">
    Levon
	</author>

	<gap:platforms>
		<gap:platform name="android" minVersion="2.1" />
		<gap:platform name="symbian" />
	</gap:platforms>
	
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/network"/>
	
	<gap:plugin name="org.apache.cordova.geolocation" />
</widget>
